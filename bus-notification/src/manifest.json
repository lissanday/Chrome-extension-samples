{
  "name": "Bus notification",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Bus",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://unpkg.com/ https://cdn.muicss.com/; object-src 'self'",
  "manifest_version": 2
}
